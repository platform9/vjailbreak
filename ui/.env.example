# vJailbreak UI Environment Variables

# Dex IdP Configuration
VITE_DEX_ISSUER=http://10.9.2.145:5556/dex
VITE_OAUTH2_PROXY_URL=/oauth2

# Kubernetes API Configuration
VITE_K8S_API_URL=https://kubernetes.default.svc.cluster.local

# Service Account Token Path (inside container)
VITE_SA_TOKEN_PATH=/var/run/secrets/kubernetes.io/serviceaccount/token

# Application Configuration
VITE_APP_NAME=vJailbreak
VITE_APP_VERSION=0.1.0

# Feature Flags
VITE_ENABLE_AUTH=true
VITE_ENABLE_PASSWORD_CHANGE=true
VITE_REQUIRE_PASSWORD_CHANGE_ON_FIRST_LOGIN=true

# Development Settings (only for local development)
VITE_DEV_MODE=false
VITE_MOCK_AUTH=false
